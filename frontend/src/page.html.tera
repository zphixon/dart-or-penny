<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf8" />
    <link rel="icon" type="image/png" href="{{ page_root }}/.dop/assets/favicon-96x96.png" sizes="96x96" />
    <link rel="icon" type="image/svg+xml" href="{{ page_root }}/.dop/assets/favicon.svg" />
    <link rel="shortcut icon" href="{{ page_root }}/.dop/assets/favicon.ico" />
    <link rel="apple-touch-icon" sizes="180x180" href="{{ page_root }}/.dop/assets/apple-touch-icon.png" />
    <meta name="apple-mobile-web-app-title" content="DartOrPenny" />
    <link rel="manifest" href="{{ page_root }}/.dop/assets/site.webmanifest" crossorigin="use-credentials" />
    <title>{{ tab_title }}</title>
    <link rel="stylesheet" href="{{ page_root }}/.dop/assets/page.css" />
    <meta name="viewport" content="width=device-width, initial-scale=1, minimal-ui">
  </head>
  <body>
    <div id="flexcontainer">
      <div id="flexinner">
        <h1>
          {% for part in page_title_parts %}
            <a href="{{ part.href }}">{{ part.path }}</a>
            {% if not loop.last %}
              {{ path_sep }}
            {% endif %}
          {% endfor %}
        </h1>
        <div id="reactRoot">
          <div class="centerme">
            <svg width="50" height="50" viewBox="0 0 24 24" xmlns="http://www.w3.org/2000/svg"><style>.spinner_P7sC{transform-origin:center;animation:spinner_svv2 .75s infinite linear}@keyframes spinner_svv2{100%{transform:rotate(360deg)}}</style><path d="M10.14,1.16a11,11,0,0,0-9,8.92A1.59,1.59,0,0,0,2.46,12,1.52,1.52,0,0,0,4.11,10.7a8,8,0,0,1,6.66-6.61A1.42,1.42,0,0,0,12,2.69h0A1.57,1.57,0,0,0,10.14,1.16Z" class="spinner_P7sC"/></svg>
          </div>
        </div>
        <div class="centerme">
          <img id="logoThing" src="{{ page_root }}/.dop/assets/apple-touch-icon.png" />
        </div>
      </div>
    </div>
    <script type="application/json" id="items">{{ items | json_encode() }}</script>
    <script type="text/plain" id="fileDir">{{ file_dir }}</script>
    <script type="text/plain" id="itemsInSubdirs">{{ in_subdirs }}</script>
    <script type="text/plain" id="pageRoot">{{ page_root }}</script>
    <script type="text/plain" id="pathSep">{{ path_sep }}</script>
    <script type="module" src="{{ page_root }}/.dop/assets/page.js"></script>
  </body>
</html>